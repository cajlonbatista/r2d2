{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",     
        "arrow-parens": "off",
        "no-multiple-empty-lines": "off",
        "no-console": "off",
        "no-useless-return": "off",
        "import/unoresolved": "off",
        "import/prefer-default-export": "off"
    }
}